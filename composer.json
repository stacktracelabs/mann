{
    "name": "stacktracelabs/mann",
    "description": "Filter package for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "StackTrace\\Mann\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StackTrace\\Mann\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
